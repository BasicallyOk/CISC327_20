{   
	"plugins": ["react", "jest"],
	"env": {
		"browser": true,
		"es2021": true,
		"jest/globals": true
	},
	"extends": [
		"standard",
		"plugin:react/recommended"
	],
	"overrides": [
	],
	"parserOptions": {
		"ecmaVersion": "latest",
		"sourceType": "module"
	},
	"rules": {
		"indent": [2, "tab"],
		"no-tabs": 0,
		"react/prop-types": 0
	}
}
